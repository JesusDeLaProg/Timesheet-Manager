{
  "name": "tm-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "lint": "tslint -p ./src/tsconfig.json",
    "build": "tsc -b ./src && cp -R ./src/public ./out",
    "build-watch": "cp -R ./src/public ./out && tsc -b --watch ./src",
    "debug": "nodemon --inspect-brk ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "inversify": "^5.0.1",
    "mongoose": "^5.6.9",
    "morgan": "~1.9.1",
    "ms": "^2.1.2",
    "reflect-metadata": "^0.1.13"
  },
  "description": "Express server for Timesheet-Manager.",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JesusDeLaProg/Timesheet-Manager.git"
  },
  "keywords": [
    "express",
    "expressjs",
    "node",
    "nodejs",
    "timesheet",
    "manager",
    "timesheet-manager"
  ],
  "author": "Maxime Charland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JesusDeLaProg/Timesheet-Manager/issues"
  },
  "homepage": "https://github.com/JesusDeLaProg/Timesheet-Manager#readme",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.12",
    "@types/morgan": "^1.7.36",
    "@types/ms": "^0.7.30",
    "@types/should": "^13.0.0",
    "mocha": "^6.2.0",
    "mongodb-memory-server": "^5.2.0",
    "nodemon": "^1.19.1",
    "should": "^13.2.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
