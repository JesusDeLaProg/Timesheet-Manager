{
  "name": "tm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "pretty": "prettier --arrow-parens always --write \"node/src/{,**/}!(*.d.ts|*.d.ts.map)\""
  },
  "dependencies": {},
  "description": "Timesheet-Manager.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JesusDeLaProg/Timesheet-Manager.git"
  },
  "keywords": [
    "timesheet",
    "manager",
    "timesheet-manager"
  ],
  "author": "Maxime Charland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JesusDeLaProg/Timesheet-Manager/issues"
  },
  "homepage": "https://github.com/JesusDeLaProg/Timesheet-Manager#readme",
  "devDependencies": {
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^2.0.2",
    "tslint": "^5.18.0",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "node/src/{,**/}!(*.d.ts.map|*.json)": [
      "tslint --fix -c ./node/tslint.json",
      "prettier --arrow-parens always --write",
      "git add"
    ],
    "angular/{,**/}{*.ts,*.html,*.scss}": [
      "prettier --config angular/.prettierrc --write",
      "git add"
    ]
  }
}
